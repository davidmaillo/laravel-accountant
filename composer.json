{
  "name": "tomirons/laravel-accountant",
  "description": "Dashboard for Stripe.",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Irons",
      "email": "tom.irons@hotmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "illuminate/contracts": "5.6.*",
    "illuminate/support": "5.6.*",
    "illuminate/pagination": "5.6.*",
    "stripe/stripe-php": "^5.1"
  },
  "require-dev": {
    "mockery/mockery": "~0.9.4",
    "phpunit/phpunit": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "TomIrons\\Accountant\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TomIrons\\Accountant\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "TomIrons\\Accountant\\AccountantServiceProvider"
      ],
      "aliases": {
        "Accountant": "TomIrons\\Accountant\\Facades\\Accountant"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
